    "joi": "^17.9.2",
    "mysql2": "^3.4.3",
    "pg": "^8.11.3",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1",
    "swagger-ui-express": "^4.6.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.3",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "cross-env": "^7.0.3",
